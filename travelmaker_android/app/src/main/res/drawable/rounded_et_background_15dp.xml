<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 배경색 설정 -->
    <solid android:color="@color/white"/>
    <!-- 테두리 설정 -->
    <stroke
        android:width="1dp"
        android:color="@color/black"/>  <!-- 원하는 색상으로 변경 가능 -->
    <!-- 모서리 둥글게 설정 -->
    <corners android:radius="15dp"/>  <!-- 원하는 모서리 둥글기 크기로 변경 가능 -->
</selector>